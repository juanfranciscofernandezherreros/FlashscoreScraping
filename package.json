{
  "name": "docker-ppt",
  "version": "1.0.0",
  "description": "a simple project for testing how to run puppeteer in a docker container",
  "main": "puppeteer-script.js",
  "type": "module",
  "scripts": {
    "test": "node src/testBasketballCsv.js && node src/testAllFunctionalities.js",
    "test:cucumber": "cucumber-js",
    "start": "node src/index.js",
    "competitions": "node src/scrapeCountries.js",
    "read-csv": "node src/readAllCsv.js",
    "results": "node src/competitionResults.cjs",
    "fixtures": "node src/competitionFixtures.cjs",
    "standings": "node src/index.js action=standings",
    "basketball": "node src/scrapeAllBasketball.js",
    "basketball:detailed": "node src/scrapeAllBasketball.js detailed=true",
    "basketball:seasons": "node src/extractLeagueSeasons.js",
    "build-links": "node src/index.js action=build-links",
    "print-csv": "node src/printCsv.js"
  },
  "keywords": [
    "puppeteer",
    "docker",
    "container"
  ],
  "author": "Apify",
  "license": "ISC",
  "dependencies": {
    "csv-parser": "^3.0.0",
    "puppeteer": "^24.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.7.0"
  }
}
